{
    "name": "rattananen/webdriver",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Jakkapong Rattananen"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-intl": "*",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "guzzlehttp/guzzle": "^7.4.5",
        "symfony/var-dumper": "^6.2",
        "symfony/process": "^6.2"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Client package for default client.",
        "symfony/process": "Process handler for remote end."
    },
    "autoload": {
        "psr-4": {
            "Rattananen\\Webdriver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rattananen\\Webdriver\\Tests\\": "tests/"
        }
    }
}